{
  "forwarders": {
    "bing": {
      "host": "www.bing.com",
      "headerTransformations": {
        "retrieve": [
            {
                "source": "/X-Special-Backend-Header-Name: (.+)/",
                "target": "'New-Header-Name: $1'"
            }
        ],
        "send": [
            {
                "source": "/X-Special-Client-Header-Name: (.+)/",
                "target": "'New-Header-Name: $1'"
            }
        ]
      }
    }
  }
}
