{
    "publicKeyPath": "",
    "privateKeyPath": "",
    "port": 8080,

    "forward": {
        "host": "www.posic.de"
    },

    "humanChecker": {
        "applicationInterfaces": {
            "base": {
                "headerName": "g-recaptcha-response",
                "secret": {
                    "__evaluate__": "environment.GOOGLE_RECAPTCHA_KEY"
                },
                "url": "https://www.google.com/recaptcha/api/siteverify?secret=${secret}&response=${clientToken}"
            },
            "googleV2-3": {}
        },
        "skipSecrets": [{
            "__evaluate__": "environment.SKIP_SECRET"
        }]
    }
}
